#include <iostream>

using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5}, i, j, target = 5;
    int arrlen = sizeof(arr) / sizeof arr[0];
    for (i = 0; i < arrlen; i++) {
        for (j = 1; j < arrlen; j++) {
            if (arr[i] + arr[j] == target) {
                cout << i << j;
                break;
            }
        }
    }
    return 0;
}
